{
  "expo": {
    "name": "KUCC-App",
    "slug": "kucc-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kucc-logo.png",
    "userInterfaceStyle": "light",
     "scheme": "kuccapp",

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/kucc-logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE"
        }
      ]
    ],

     "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kucc.kuccapp",
       "associatedDomains": [
        "applinks:ahilsdrlrziqhziuaygf.supabase.co"
      ]
    },
    
    "android": {
      "package": "com.kucc.kuccapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/kucc-logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
               "scheme": "https",
              "host": "ahilsdrlrziqhziuaygf.supabase.co",
              "pathPrefix": "/auth/v1/verify"
            },

            {
              "scheme": "kuccapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },

    "extra": {
      "router": {
      "origin": false
    },
      "eas": {
        "projectId": "45e34849-d704-4045-bf5d-a81e94f03429"
      }
    }
  }
}